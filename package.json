{
  "name": "advanced-react",
  "version": "1.0.0",
  "main": "lib/server.js",
  "author": "Troy Nichols",
  "license": "MIT",
  "scripts": {
    "server-dev": "NODE_PATH='./lib' pm2 startOrRestart pm2.config.json",
    "server-prod": "babel lib -d build --copy-files",
    "server-down": "pm2 kill",
    "web-dev": "webpack -wd",
    "web-prod": "webpack -p",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cover": "jest --coverage"
  },
  "babel": {
    "presets": [
      "react",
      ["env", {
        "targets": {"node": "current"}
      }]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "react-test-renderer": "^16.3.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^2.10.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
